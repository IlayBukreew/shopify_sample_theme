#shopify-section-cart-template {
    padding-bottom: 131px;
    width: 100%;
    max-width: 1180px;
    margin: 0 auto;

    h1 {
        font-weight: 700;
        line-height: 64px;
        font-size: 46px;
        text-align: center;
        color: #1D4668;
        margin-top: 82px;
        margin-bottom: 31px;
    }

    .cart {
        display: flex;
        flex-direction: row;
        justify-content: space-between;

        .wrap_cart {
            padding-top: 9px;
            width: 65%;
            padding-right: 40px;

            table {
                width: 100%;

                tbody {
                    box-shadow: inset 0px -1px 0px rgba(0, 0, 0, 0.06484);
                }

                tbody tr {
                    box-shadow: inset 0px 1px 0px rgba(0, 0, 0, 0.0648494);
                }

                th {
                    font-weight: 300;
                    line-height: 23px;
                    font-size: 13px;
                    color: #3B3C43;
                    text-align: left;
                }
            }
        }

        .cart__image {
            width: 87%;
            margin: 10px 0;
        }

        .cart__image-wrapper {
            width: 15%;
        }

        .list-view-item__title a {
            font-weight: 500;
            line-height: 28px;
            font-size: 16px;
            color: #3B3C43;
            text-decoration: none;
        }

        .cart__meta-text {
            font-style: normal;
            font-weight: normal;
            line-height: 23px;
            font-size: 13px;
            color: #3B3C43;
            opacity: 0.5;
        }

        .remove a {
            text-decoration: none;
        }

        .cart__price-wrapper {
            font-weight: 500;
            line-height: 28px;
            font-size: 16px;
            color: #3B3C43;
        }

        .cart-note {
            margin-top: 46px;

            .note {
                textarea {
                    height: 0;
                    margin-left: auto;
                    display: block;
                    margin-top: 20px;
                    width: 100%;
                    opacity: 0;
                    padding: 20px;
                    background: #dedede;
                    color: black;
                    border: none;
                    font-family: "Poppins";
                    box-shadow: 1px 1px 5px 0px grey;
                    transition: opacity .5s ease, height .5s ease;

                    &:focus {
                        border: none;
                        outline: none;
                    }
                }

                &.open {
                    textarea {
                        height: 100px;
                        min-height: 100px;
                        opacity: 1;
                    }
                }
            }
        }

        .cart-note a {
            font-weight: 500;
            line-height: 24px;
            font-size: 14px;
            text-align: center;
            text-decoration: none;
            color: #1D4668;
        }

        .add_note {
            float: right;
            font-weight: 300;
            line-height: 23px;
            font-size: 13px;
            color: #3B3C43;
            cursor: pointer;
            border-bottom: 2px dotted #c3c3c3;
        }

        .cart__footer {
            max-width: 35%;
            width: 100%;

            .grid {
                background: #F9F7EF;
                font-weight: 300;
                line-height: 25px;
                font-size: 15px;
                color: #727277;

                .grid__item {
                    padding: 27px;
                }

                span {
                    margin-bottom: 19px;
                    display: inline-block;
                }

                .cart__submit {
                    cursor: pointer;
                    border: none;
                    margin: 18px 0;
                    font-weight: normal;
                    line-height: normal;
                    font-size: 15px;
                    text-align: center;
                    color: #FFFFFF;
                    background: #178C4D;
                    box-shadow: 0px 5px 20px rgba(32, 146, 85, 0.5), 0px 3px 8px rgba(32, 146, 85, 0.2);
                    border-radius: 27.5px;
                    width: 100%;
                    padding: 15px 0;
                    font-weight: 600;
                    transition: background .4s ease;

                    &:hover {
                        background: #48b77b;
                    }
                }

                .cart__subtotal {
                    font-weight: 600;
                    line-height: 25px;
                    font-size: 15px;
                    color: #F36060;
                    float: right;
                }

                .cart__subtotal-title {
                    color: #444444;
                    font-size: 20px;
                    font-weight: 500;
                }
            }
        }
    }

    .quantity {
        margin-left: 20px;
        background: rebeccapurple;
        padding: 7px 10px;
        display: flex;
        align-items: center;
        justify-content: space-between;
        border-radius: 22px;
        background: #fbfbfb;
        border: 1px solid #edecec;
        /* margin: 0 auto; */
        max-width: 65px;
        margin-right: auto;
        margin-left: -2px;

        div {
            color: #8f8f8f;
            cursor: pointer;
        }

        input {
            width: 40px;
            height: 30px;
            background: #fbfbfb;
            border: none;
            text-align: center;
            color: #8f8f8f;
            font-size: 16px;
            font-weight: 500;
        }
    }

    .payp {
        border-radius: 27.5px;
        overflow: hidden;

        .additional-checkout-button--paypal {
            width: 100%;
        }
    }
}

