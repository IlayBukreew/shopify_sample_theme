            }
        }

        h2 {
            padding: 0 38px;
            font-weight: 700;
            line-height: 32px;
            text-align: center;
            color: #1D4668;
            font-size: 34px;
            margin-top: 43px;
            order: 2;
        }

        .products {
            text-align: center;
            max-width: 1184px;
            margin: 0 auto;
            display: flex;
            align-items: flex-start;
            justify-content: space-between;
            margin-top: 145px;

            img {
                width: 57%;
                margin-top: -100px;
            }

            .disc {
                font-weight: 300;
                text-align: left;
                line-height: 28px;
                font-size: 16px;
                padding: 0 38px;
