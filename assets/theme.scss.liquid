                    a {
                        text-decoration: none;
                        font-weight: 500;
                        line-height: 28px;
                        font-size: 16px;
                        color: #484A40;
                    }

                    &::after {
                        content: '';
                        position: absolute;
                        left: 50%;
                        bottom: -38px;
                        width: 0;
                        height: 4px;
                        display: block;
                        background: #178C4D;
                        transition: width .5s ease, left .5s ease;
                    }

                    &:hover {
                        &::after {
                            width: 100%;
                            left: 0;
                        }
                    }
                }
            }
