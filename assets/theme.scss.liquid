.page-width {
    display: flex;
    flex-direction: column;
    margin: 0 auto;

    section {
        order: 3;
        text-align: left;
        max-width: 1184px;
        margin: 0 auto;

        h1,
        h2 {
            font-weight: 700;
            line-height: 64px;
            font-size: 46px;
            text-align: center;
            color: #1D4668;
            margin-top: 80px;
        }

        h3 {
            font-weight: 700;
            line-height: 69px;
            font-size: 36px;
            text-align: center;
            color: #1D4668;
            margin-top: 80px;
            max-width: 666px;
            margin: 0 auto;
        }

        p {
            font-weight: 300;
            line-height: 36px;
            font-size: 18px;
            color: #838383;
            margin: 0 auto;
            margin-top: 24px;
        }

        &:first-of-type {
            padding-bottom: 86px;
            background-color: #F9F7EF;
            text-align: center;
            order: 0;
            max-width: unset;

            p {
                max-width: 940px;
            }
        }
    }

    .grid {
        display: flex;
        flex-direction: column;
        width: 100%;

        .wrapper {
            order: 2;

            .numbers {
                display: flex;
                align-items: flex-start;
                justify-content: space-between;
                max-width: 589px;
                margin: 0px auto;
                margin-bottom: 60px;
                margin-top: -26px;

                .number {
                    width: 100px;
                    display: flex;
                    flex-direction: column;
                    align-items: flex-start;

                    span {
                        font-weight: 600;
                        line-height: 32px;
                        font-size: 19px;
                        color: #FFFFFF;
                        width: 50px;
                        height: 50px;
                        display: inline-flex;
                        align-items: center;
                        justify-content: center;
                        border-radius: 52px;
                        background: #B1D421;
                        margin-left: -27px;
                        margin-top: 1px;
                        border: 4px solid white;
                        box-shadow: 0 0 0 1px #B1D421;
                    }

                    &:nth-child(2) {
                        display: flex;
                        flex-direction: column;
                        align-items: flex-end;

