            .cart {
                margin-bottom: 9px;
                position: relative;

                span {
                    position: absolute;
                    background: #F36060;
                    width: 19px;
                    height: 19px;
                    display: flex;
                    align-items: center;
                    justify-content: center;
                    border-radius: 39px;
                    font-weight: normal;
                    line-height: 18px;
                    font-size: 10px;
                    text-align: center;
                    color: #FFFFFF;
                    top: -6px;
                    right: -3px;

                    &.hide {
                        display: none;
                    }
                }

                a {
                    background: white;
                    display: flex;
                    align-items: center;
                    justify-content: center;
                    padding: 10px 11px;
                    border-radius: 30px;
                    box-shadow: 0 0 16px -3px #00000073;
                }
            }

            .mob_trigger {
                display: none;
                margin-bottom: 9px;

                svg {
                    background: white;
                    display: flex;
                    align-items: center;
                    justify-content: center;
                    padding: 15px;
                    border-radius: 30px;
                    box-shadow: 0 0 16px -3px #00000073;
                }
            }
        }
    }

    .hero {
        padding-top: 10px;
        max-width: 1175px;
        margin: 0 auto;
        display: flex;
        padding-bottom: 107px;
        position: relative;

        .btn {
            background: #178C4D;
            text-decoration: none;
            color: white;
            padding: 15px 55px 15px 34px;
            display: inline-block;
            box-shadow: 0px 5px 20px rgba(32, 146, 85, 0.5), 0px 3px 8px rgba(32, 146, 85, 0.2);
            border-radius: 27.5px;
            font-weight: 700;
            line-height: normal;
            font-size: 15px;
            margin-top: 17px;
            transition: background .4s ease;

            &:hover {
                background: #48b77b;
            }
        }

        .left {
            padding-top: 65px;
            padding-left: 3px;
            position: relative;
            width: 51%;
            z-index: 3;

            .title {
                font-family: 'Cookie', cursive;
                ;
                font-weight: normal;
                line-height: 58px;
                font-size: 95px;
                color: #178C4D;
                font-size: 165px;
                margin-top: 36px;
            }

            svg {
                margin-left: 15px;
                margin-top: 19px;
            }

            h2 {
                color: white;
                margin-top: 39px;
                font-style: normal;
                font-weight: 700;
                line-height: 70px;
                font-size: 69px;
            }

            p {
                font-size: 18px;
                margin-top: 4px;
                line-height: 30px;
                margin-bottom: 18px;
                font-weight: 300;
            }
        }

        .right {
            width: 49%;
            position: relative;

            .btn {
                display: none;
            }

            .slider {
                display: flex;
                align-items: stretch;
                position: relative;
                user-select: none;
                overflow: hidden;
                width: 100%;

                .control {
                    position: absolute;
                    top: 44%;
                    right: 0;
                    background: white;
                    width: 53px;
                    height: 53px;
                    border-radius: 42px;
                    border: none;
                    cursor: pointer;
                    z-index: 1;
                    opacity: 1;
                    transition: opacity .1s ease;

                    &.prev {
                        left: 0;
                    }

                    &.hide {
                        opacity: 0;
                    }
                }

                .slides {
                    list-style-type: none;
                    white-space: nowrap;
                    font-size: 0;
                    line-height: 0;
                    padding-top: 8px;
                    transition: transform .5s ease;

                    img {
                        width: 100%;
                        opacity: 0;
                        transition: opacity 1.5s ease;

                        &.show {
                            opacity: 1;
                        }
                    }
                }
            }
        }
    }
}

