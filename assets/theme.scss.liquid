    }

    .navbar {
        display: flex;
        justify-content: space-between;
        max-width: 1175px;
        margin: 0 auto;
        padding-bottom: 19px;
        align-items: flex-start;
        padding-top: 34px;
        border-bottom: 1px solid rgba(255, 255, 255, 0.658);

        .nav {
            width: 100%;
            padding-left: 87px;
            padding-right: 45px;
