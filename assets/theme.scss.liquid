html {
    overflow-x: hidden;
}

* {
    padding: 0;
    margin: 0;
}

.lazyload,
.lazyloading {
    opacity: 0;
}

.lazyloaded {
    opacity: 1;
    transition: opacity 400ms;
}

::-webkit-scrollbar-thumb {
    background-color: #e0e0e0;
}

::-webkit-scrollbar-track {
    background: white;
}

::-webkit-scrollbar {
    background: white;
    height: 11px;
}

.swiper-container {
    width: 100%;
    height: 100%;
}

.ajax_msg {
    display: flex;
    align-items: center;
    position: fixed;
    top: -60px;
    left: 0;
    width: 100%;
    min-height: 54px;
    background: #3e3e3e;
    z-index: 100;
    transition: top .5s ease;

    &.added {
        top: 0;
    }

    .wrap {
        max-width: 560px;
        text-align: center;
        margin: 0 auto;
        padding: 0 20px;
        color: #57d891;

        a {
            display: block;
            color: #57d891;
        }

        span {
            color: white;
        }
    }
}

.swiper-slide {
    text-align: center;
    font-size: 18px;
    background: #fff;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
}

body {
    font-family: 'Poppins', sans-serif;
}

